{
  "name": "markee-subgraph",
  "version": "0.1.0",
  "description": "The Graph subgraph for Markee protocol",
  "repository": "https://github.com/1Hive/markee",
  "author": "Markee Team",
  "license": "GPL-3.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy:optimism": "graph deploy --studio markee-optimism",
    "deploy:base": "graph deploy --studio markee-base",
    "deploy:arbitrum": "graph deploy --studio markee-arbitrum",
    "deploy:mainnet": "graph deploy --studio markee-mainnet",
    "test": "graph test",
    "create-local": "graph create --node http://localhost:8020/ markee",
    "remove-local": "graph remove --node http://localhost:8020/ markee",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 markee",
    "convert-abis": "node convert-abis.js"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.56.0",
    "@graphprotocol/graph-ts": "^0.31.0"
  },
  "devDependencies": {
    "matchstick-as": "^0.5.0"
  }
}
