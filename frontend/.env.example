# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Alchemy API Keys (for RPC connections)
NEXT_PUBLIC_ALCHEMY_MAINNET_KEY=your_alchemy_mainnet_key
NEXT_PUBLIC_ALCHEMY_OPTIMISM_KEY=your_alchemy_optimism_key
NEXT_PUBLIC_ALCHEMY_ARBITRUM_KEY=your_alchemy_arbitrum_key
NEXT_PUBLIC_ALCHEMY_POLYGON_KEY=your_alchemy_polygon_key
NEXT_PUBLIC_ALCHEMY_BASE_KEY=your_alchemy_base_key

# WalletConnect Project ID
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Contract Addresses (will be populated after deployment)
NEXT_PUBLIC_FACTORY_ADDRESS_MAINNET=
NEXT_PUBLIC_FACTORY_ADDRESS_OPTIMISM=
NEXT_PUBLIC_FACTORY_ADDRESS_ARBITRUM=
NEXT_PUBLIC_FACTORY_ADDRESS_POLYGON=
NEXT_PUBLIC_FACTORY_ADDRESS_BASE=
NEXT_PUBLIC_FACTORY_ADDRESS_GNOSIS=
NEXT_PUBLIC_FACTORY_ADDRESS_CELO=

# Development
NODE_ENV=development
NEXT_PUBLIC_DEBUG=false
